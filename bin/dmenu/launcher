#!/bin/sh
case "$(echo -e "ﮣ Powermenu\nﲃ Emoji\n直 Wifi\n Screenshot\n Bluetooth" | dmenu \
    -i -c -p\
    "  Widgets: " -h 36 -l 5)" in
        "ﮣ Powermenu") exec ~/.local/bin/dmenu/powermenu;;
        "ﲃ Emoji") exec ~/.local/bin/dmenu/emojimenu;;
        "直 Wifi") exec ~/.local/bin/dmenu/wifimenu;;
        " Screenshot") ~/.local/bin/dmenu/ssmenu;;
        " Bluetooth") exec ~/.local/bin/dmenu/bluetoothmenu;;
esac
